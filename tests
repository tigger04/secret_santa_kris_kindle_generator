source kriskindle
# iteration=$EPOCHSECONDS.$RANDOM.$RANDOM.$RANDOM

setup () {

    cat - >testdata <<eom
Data -Spot -Yarr
"La Forge" =Crusher -Worf
Worf -Troy
Troy -Worf
Yarr -Data
eom
}

@test "test data loaded" {
    [ $(wc -l < testdata) -gt 4 ]
}

